
img {
   -webkit-animation: myAnimation 1s ; /* Chrome, Safari, Opera */
    animation: myAnimation 1s ;
}
 /* Chrome, Safari, Opera */
  @-webkit-keyframes myAnimation {
    from {opacity: 0.2;}
    to {opacity: 1;}
  }
  
  keyframes myAnimation {
    from {opacity: 0.2;}
    to {opacity: 1;}
  }
  
